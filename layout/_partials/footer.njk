{% if theme.footer.copyright !== false %}
<span class="copyright">
  {% set copyright_year = date(null, 'YYYY') %}
  Copyright &copy;
  {% if theme.footer.since and theme.footer.since != copyright_year %}{{ theme.footer.since }} â€“ {% endif %}
  <span itemprop="copyrightYear">{{ copyright_year }}</span>
  <span class="author" itemprop="copyrightHolder">{{ theme.footer.copyright or config.author }}</span>.
  All Rights Reserved
</span>
{% if theme.footer.github %}
<div style="flex-grow: 1;"></div>
<a href="{{ theme.footer.github }}" target="_blank" rel="noopener noreferrer">GitHub</a>
{% endif %}
{% endif %}
<script src="{{ url_for(theme.js) }}/boot.js"></script>
<script src="{{ url_for(theme.js) }}/third-party/pjax.js"></script>
<script src="{{ url_for(theme.js) }}/pjax.js"></script>